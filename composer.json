{
	"name": "lasserafn/php-fik",
	"description": "Generate danish FIK numbers easily.",
	"keywords": [
		"LasseRafn",
		"FIK",
		"invoice",
		"reminder",
		"accounting"
	],
	"homepage": "https://github.com/LasseRafn/FIK",
	"license": "MIT",
	"authors": [
		{
			"name": "Lasse Rafn",
			"email": "lasserafn@gmail.com",
			"homepage": "https://apricot.dk",
			"role": "Developer"
		}
	],
	"require": {
		"php": "^5.6|^7.0|^7.1"
	},
	"require-dev": {
		"phpunit/phpunit": "^5.7",
		"satooshi/php-coveralls": "^1.0"
	},
	"autoload": {
		"psr-4": {
			"LasseRafn\\FIK\\": "src"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"LasseRafn\\FIK\\Test\\": "tests"
		}
	},
	"scripts": {
		"test": "vendor/bin/phpunit tests"
	},
	"config": {
		"sort-packages": true
	}
}
